{
  "name": "youtube-video-js",
  "version": "1.6.2",
  "description": "Easily play and control Youtube video using javascript",
  "author": "Mark Kennedy <mark@heyimmark.com> (heyimmark.com)",
  "homepage": "https://github.com/mkay581/youtube-video-js#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mkay581/youtube-video-js.git"
  },
  "engines": {
    "node": ">=8.11.0"
  },
  "license": "MIT",
  "keywords": [
    "video",
    "social media",
    "youtube",
    "youtube video",
    "social video"
  ],
  "main": "./src/youtube-video.js",
  "scripts": {
    "test": "karma start karma.conf.js && eslint ./",
    "preversion": "npm test",
    "minify": "uglifyjs dist/youtube-video.js -o dist/youtube-video.min.js",
    "build-dist": "rm -rf dist/* && rollup src/youtube-video.js --format umd --name YoutubeVideo --file dist/youtube-video.js && npm run minify",
    "version": "npm run build-dist && banner-cli dist/*.js && git add -A dist",
    "postversion": "git push && git push --tags",
    "start": "bt server"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "assert": "^1.4.1",
    "banner-cli": "0.11.1",
    "chai": "^4.1.2",
    "eslint": "^5.5.0",
    "karma": "^3.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^6.0.1",
    "mocha": "^5.2.0",
    "rollup": "^0.65.2",
    "sinon": "^6.2.0",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "resource-manager-js": "^5.0.5"
  }
}
